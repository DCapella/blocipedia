<h1>New Collaborator</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for @collaborator do |f| %>
      <div class="form-group">
        <%= f.label 'Collaborators' %>
        <%= f.text_field :email, class: 'form-control', placeholder: "Enter collaborator email" %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
